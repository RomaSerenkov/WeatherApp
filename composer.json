{
    "name": "serenkov/localhost",
    "authors": [
        {
            "name": "Roman Serenkov",
            "email": "serenkovroma@mail.ru"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.5",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Vendor\\Namespace\\": ""
        },
        "psr-0": {
            "Monolog": "src/",
            "Vendor\\Namespace": [
                "src/"
            ],
            "Pear_Style": "src/",
            "": "src/"
        }
    },
    "scripts": {
        "cs-fix": [
            "php-cs-fixer fix src --rules=@PSR2"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/process": "^4.4",
        "symfony/console": "^4.4",
        "phpstan/phpstan": "^0.12.57",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.4"
    }
}
